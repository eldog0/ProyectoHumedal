---
import Layout from '../layouts/Layout.astro';
import Navbar from '../components/Navbar.astro'
import Hero from '../components/Hero.astro'
import EspeciesSection from '../components/EspeciesSection.astro';
import Conservacion from '../components/Conservacion.astro'
import Footer from '../components/Footer.astro'

const consejos = [
 {
    title: "Respeta la flora y fauna local",
    description: "Evita alterar el entorno natural de los humedales. No toques las plantas ni molestes a los animales que habitan en estos ecosistemas, ya que cada especie cumple una función vital en el equilibrio ecológico.",
    imageUrl: "data:image/jpeg;base64,"
  },
  {
	tittle: "No abandones animales",
    description: "Abandodar mascotas u otro animales no pertencientes al ecositema, pone en riesgo a la flora y fauna prpia del humdeal.",
    imageUrl: "https://st2.depositphotos.com/4177785/43156/v/450/depositphotos_431560872-stock-illustration-invasive-species-red-gradient-concept.jpg"
  },
  {
    title: "No dejes basuera",
    description: "Siempre lleva tus desechos de vuelta contigo. La contaminación de los humedales puede afectar la calidad del agua y poner en peligro a las especies locales. Utiliza los basureros disponibles o lleva una bolsa para recoger tu basura.",
    imageUrl: "data:image/png;base64,"
  },
  {
    title: "Promueve la educacion ambiental",
    description: "Informa a otras personas sobre la importancia de los humedales. La conciencia pública es fundamental para la conservación de estos ecosistemas vitales. Participa en actividades educativas o programas de conservación",
    imageUrl: "https://example.com/flora.jpg"
  },
  {
    title: "Apoya Proyectos de Conservacion",
    description: "Si tienes la oportunidad, apoya proyectos y organizaciones dedicadas a la protección de los humedales. Ya sea a través de donaciones, voluntariado o difusión, tu ayuda puede marcar la diferencia.",
    imageUrl: "data:image/jpeg;base64,"
  },
];
---

<Layout title="Humedal Bayona">
	<main>
		<Navbar />
		<Hero />
		<EspeciesSection />
		<section id="consejos">
			{consejos.map(consejo => (
			  <Conservacion {...consejo} />
			))}
		  </section>
		<Footer />
	</main>
</Layout>


